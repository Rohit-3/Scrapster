# Google API Configuration
# Copy this file to config.env and fill in your credentials

# Option 1: Single API Key (for basic use)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CSE_ID=your_custom_search_engine_id_here

# Option 2: Multiple API Keys (for unlimited scraping)
# Format: key1:cse_id1,key2:cse_id2,key3:cse_id3
# Example: AIzaSy123:abc123,AIzaSy456:def456,AIzaSy789:ghi789
MULTIPLE_API_KEYS=

# Auto-rotate API keys when quota is reached (true/false)
AUTO_ROTATE_KEYS=false

# Rate limiting (seconds between requests)
RATE_LIMIT_DELAY=1

